
library = sk.sys ;

objects = 
  Process.cc
  AbstractProcessListener.cc
  DelegatingExecutable.cc
  PipeProcess.cc
  PtyProcess.cc
  Pipeline.cc
  Group.cc
  User.cc
;

make-library $(library) : $(objects) ;
make-subdir test ;

probes = 
  user-probe
;
use-project-library $(probes) : sk.sys sk.io sk.util sk.rt.bundle ;

local _probe ;
for _probe in $(probes) {
  make-program $(_probe) : $(_probe).cc $(objects) ;
}
